{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.cjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.cjs"
        }
    ],
    "crons": [
        {
            "path": "/api/test/scrape",
            "schedule": "0 0 * * *"
        }
    ]
}