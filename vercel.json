{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "crons": [
        {
            "path": "/api/test/scrape",
            "schedule": "0 0 * * *"
        }
    ]
}